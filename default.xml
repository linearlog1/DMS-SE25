<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <default sync-j="2" revision="DMS-SE25"/>

  <remote fetch="git://git.yoctoproject.org" name="yocto"/>
  <remote fetch="git://github.com/Freescale" name="freescale"/>
  <remote fetch="git://git.openembedded.org" name="oe"/>
  <remote fetch="git://github.com/OSSystems" name="OSSystems"/>
  <remote fetch="git://github.com/meta-qt5"  name="QT5"/>
  <remote fetch="git://git.freescale.com/imx" name="fsl-release" />
  <remote fetch="git://github.com/linearlog1/" name="advanserver"/>
  <project remote="yocto" name="poky" path="sources/poky"/>
  <project remote="advanserver" name="meta-freescale" path="sources/meta-freescale"/>

  <project remote="advanserver" name="meta-openembedded" path="sources/meta-openembedded"/>

  <project remote="advanserver" name="fsl-community-bsp-base" path="sources/base">
      <linkfile dest="README" src="README"/>
      <linkfile dest="setup-environment" src="setup-environment"/>
  </project>

  <project remote="advanserver" name="meta-freescale-3rdparty" path="sources/meta-freescale-3rdparty"/>
  <project remote="advanserver" name="meta-freescale-distro" path="sources/meta-freescale-distro"/>
  <project remote="advanserver" name="meta-advantech-arm-extra" path="sources/meta-advantech-arm-extra" />

  <project remote="advanserver" name="meta-browser" path="sources/meta-browser" />
  <project remote="advanserver" name="meta-qt5" path="sources/meta-qt5" />

  <project remote="advanserver" name="meta-fsl-bsp-release" path="sources/meta-fsl-bsp-release">
      <linkfile src="imx/tools/fsl-setup-release.sh" dest="fsl-setup-release.sh"/>
      <linkfile src="imx/README" dest="README-IMXBSP"/>
  </project>

</manifest>
